<xsd:schema xmlns:test="schemaBindings/nameXmlTransform"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    targetNamespace="schemaBindings/nameXmlTransform" jaxb:version="2.0">
    <xsd:annotation>
        <xsd:appinfo>
            <jaxb:schemaBindings>
                <jaxb:nameXmlTransform>
                    <jaxb:elementName suffix="ElementSuf" prefix="ElementPref" />
                </jaxb:nameXmlTransform>
            </jaxb:schemaBindings>
        </xsd:appinfo>
    </xsd:annotation>

    <xsd:complexType name="RootKind">
        <xsd:sequence>
            <xsd:element name="child" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="attr1" type="xsd:string" use="required"
                        fixed="value" />
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="test:child2" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="root" type="test:RootKind" />

    <xsd:element name="child2">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="child3" type="xsd:string" />
                <xsd:element ref="test:root" minOccurs="0" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
