<xsd:schema xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:test="globalBindings/collectionType"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" jaxb:version="2.0"
	targetNamespace="globalBindings/collectionType">
	<xsd:annotation>
		<xsd:appinfo>
			<jaxb:globalBindings collectionType="indexed" />
		</xsd:appinfo>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>
		Test verifies generation of collection property as array.
		</xsd:documentation>
	</xsd:annotation>

	<xsd:element name="root" >
		<xsd:complexType >
			<xsd:choice minOccurs="1" maxOccurs="unbounded">
				<xsd:element name="date" type="xsd:date" />
				<xsd:element name="time" type="xsd:time"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
