<TestGroup ID="name00805">
    <TestGroupDocumentation>
        <Title><![CDATA[Element names contain only punctuation characters and digits. (valid schema)]]></Title>
        <Description><![CDATA[
        <p>
                    Declare an element with a sequence of two integer elements. Name the first one
                                     as "_-." and the second one as "_-0.". 
                             
                                     The document name00805_p.xml sets the attributes to 0 and 1 respectively.
        </p>
                    
        
        <p>
          <a href="http://www.w3.org/TR/2001/REC-xmlschema-1-20010502/#cElement_Declarations">
            http://www.w3.org/TR/2001/REC-xmlschema-1-20010502/#cElement_Declarations
          </a>
        </p>
    ]]></Description>
    </TestGroupDocumentation>
    <TestGroupAttributes>
        <AttrElem Name="scInfo">@(#)name00805.test.xml	1.2 08/01/24</AttrElem>
        <AttrElem Name="testType">JAXBXMLSchemaTest</AttrElem>
        <TargetSpec Version="1.0-" ID="XMLSchema"/>
    </TestGroupAttributes>
    <CodeSet>
        <Dependency>
            <Lib>jaxb.test</Lib>
        </Dependency>
        <ExternalSupportClass SourceName="../name00805.xsd"/>
        <ExternalData Type="iodata" SourceName="name00805.sig"/>
    </CodeSet>
    <TestCase ID="name00805">
        <TestCaseAttributes>
            <AttrElem Name="validity">1</AttrElem>
        </TestCaseAttributes>
    </TestCase>
