<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="ElemDecl/name"
    targetNamespace="ElemDecl/name">

    <xsd:element name="root">
        <xsd:complexType>
            <xsd:sequence>
                <!-- element names have punctuation characters -->
                <xsd:element ref="a--a"/>
                <xsd:element ref="b&#x002E;&#x002E;b"/>
                <xsd:element ref="c__c"/>
                <xsd:element ref="d&#x00B7;&#x00B7;d"/>
                <xsd:element ref="e&#x0387;&#x0387;e"/>
                <xsd:element ref="f&#x06DD;&#x06DD;f"/>
                <xsd:element ref="g&#x06DE;&#x06DE;g"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="a--a" type="xsd:int"/>
    <xsd:element name="b&#x002E;&#x002E;b" type="xsd:int"/>
    <xsd:element name="c__c" type="xsd:int"/>
    <xsd:element name="d&#x00B7;&#x00B7;d" type="xsd:int"/>
    <xsd:element name="e&#x0387;&#x0387;e" type="xsd:int"/>
    <xsd:element name="f&#x06DD;&#x06DD;f" type="xsd:int"/>
    <xsd:element name="g&#x06DE;&#x06DE;g" type="xsd:int"/>

</xsd:schema>
