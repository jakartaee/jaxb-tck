         <Lib>xml_schema.TestRun</Lib>
      </Dependency>
      <ExternalSupportClass SourceName="../name00804.xsd"/>
   </CodeSet>
   <TestCase ID="name00804">
      <TestCaseAttributes>
         <AttrElem Name="validity">1</AttrElem>
      </TestCaseAttributes>
   </TestCase>
   <TestCase ID="name00804_p">
      <TestCaseAttributes>
         <AttrElem Name="validity">1</AttrElem>
      </TestCaseAttributes>
      <CodeSet>
         <ExternalData SourceName="../name00804_p.xml" Type="iodata"/>
      </CodeSet>
   </TestCase>
</TestGroup>
