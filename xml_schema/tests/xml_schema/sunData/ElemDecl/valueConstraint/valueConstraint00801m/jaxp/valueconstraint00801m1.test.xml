<TestGroup ID="valueconstraint00801m1">
   <TestGroupDocumentation>
      <Title><![CDATA[value of mixed content type must match the fixed value (valid schema)]]></Title>
      <Description><![CDATA[
        <p>
                    If the {content type} of the actual type definition is mixed, 
                             then the initial value of the item must match the canonical lexical 
                             representation of the {value constraint} value.
                              In the test, the value of the item does not match the
                             specified fixed value.
        </p>
                    
        
        <p>
          <a href="http://www.w3.org/TR/2001/REC-xmlschema-1-20010502/#cElement_Declarations">
            http://www.w3.org/TR/2001/REC-xmlschema-1-20010502/#cElement_Declarations
          </a>
        </p>
    ]]></Description>
   </TestGroupDocumentation>
   <TestGroupAttributes>
      <AttrElem Name="scInfo">@(#)valueconstraint00801m1.test.xml	1.1 07/03/27</AttrElem>
      <AttrElem Name="testType">XMLSchemaTest</AttrElem>
      <TargetSpec ID="XMLSchema" Version="1.0-"/>
      <Keyword>runtime</Keyword>
   </TestGroupAttributes>
   <CodeSet>
      <Dependency>
         <Lib>xml_schema.TestRun</Lib>
      </Dependency>
      <ExternalSupportClass SourceName="../valueConstraint00801m1.xsd"/>
   </CodeSet>
   <TestCase ID="valueconstraint00801m1">
      <TestCaseAttributes>
         <AttrElem Name="validity">1</AttrElem>
      </TestCaseAttributes>
   </TestCase>
   <TestCase ID="Positive">
